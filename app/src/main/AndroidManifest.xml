<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="co.obware.hashcode">

    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />
    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/logo"
        android:label="@string/app_name"
        android:roundIcon="@drawable/logo"
        android:supportsRtl="true"
        android:theme="@style/AppTheme"
        tools:ignore="GoogleAppIndexingWarning">
        <activity android:name=".activity.SipActivity"
            android:parentActivityName=".activity.DashBoardActivity"/>
        <activity android:name=".activity.ViewComplaintActivity"
            android:parentActivityName=".activity.DashBoardActivity"/>
        <activity android:name=".activity.ComplaintActivity"
            android:parentActivityName=".activity.DashBoardActivity"/>
        <activity
            android:name=".activity.EnoticeActivity"
            android:parentActivityName=".activity.DashBoardActivity" />
        <activity
            android:name=".activity.ForgetActivity"
            android:parentActivityName=".activity.LoginActivity" />
        <activity android:name=".activity.DashBoardActivity" />
        <activity android:name=".activity.LoginActivity" />
        <activity android:name=".activity.MainActivity" />
        <activity android:name=".activity.ChatActivity"
            android:parentActivityName=".activity.DashBoardActivity"/>
        <activity android:name=".SplashActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".activity.RegisterActivity"
            android:parentActivityName=".activity.LoginActivity" />
        <activity android:name=".activity.LibraryActivity"
            android:parentActivityName=".activity.DashBoardActivity"/>
        <activity android:name=".activity.ColtActivity"
            android:parentActivityName=".activity.DashBoardActivity"/>
        <activity android:name=".activity.EditProfile"
            android:parentActivityName=".activity.DashBoardActivity"/>
    </application>

</manifest>